---
import Card from '../components/Card.astro';
import Layout from '../layouts/Layout.astro';
import { getCollection } from 'astro:content';

const events = await getCollection('events');
---

<Layout title="わにサークル">
	<main>
    <h1>わにサークルのイベント情報</h1>
		<ul>
			{events.map((event) => (
				<Card event={event} />
			))}	
    	</ul>
	</main>
</Layout>